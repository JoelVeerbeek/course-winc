        "Creating graphs to visualize the data"
      ],
      "metadata": {
        "id": "th8719Rd7CKB"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "fig, (ax1, ax2, ax3, ax4) = plt.subplots(1, 4, figsize=(20, 3), sharey=False)\n",
        "\n",
        "# plot for CO2 emissies per country since 1980\n",
        "for c in country:\n",
        "    subset = filter_co2[filter_co2[\"country\"] == c]\n",
        "    ax1.plot(subset[\"year\"], subset['co2_per_capita'], label=c)\n",
        "\n",
        "ax1.set_title(\"CO2 emissies per capita since 1980\")\n",
        "ax1.set_xlabel(\"Year\")\n",
        "ax1.set_ylabel(\"CO2 emissies\")\n",
        "ax1.legend(loc=\"upper center\", bbox_to_anchor=(0.5, -0.20), ncol=3)\n",
        "ax1.grid(True)\n",
        "\n",
        "# plot for Coal CO2 per capita since 1980\n",
        "for c in country:\n",
        "    subset = filter_co2[filter_co2[\"country\"] == c]\n",
        "    ax2.plot(subset[\"year\"], subset['coal_co2_per_capita'], label=c)\n",
        "\n",
        "ax2.set_title(\"Coal CO2 per capita since 1980\")\n",
        "ax2.set_xlabel(\"Year\")\n",
        "ax2.set_ylabel(\"coal CO2\")\n",
        "ax2.legend(loc=\"upper center\", bbox_to_anchor=(0.5, -0.20), ncol=3)\n",
        "ax2.grid(True)\n",
        "\n",
        "# plot for GDP per capita since 1980\n",
        "for c in country:\n",
        "    subset = filter_co2[filter_co2[\"country\"] == c]\n",
        "    ax3.plot(subset[\"year\"], subset['gdp_per_capita'], label=c)\n",
        "\n",
        "ax3.set_title(\"GDP per capita since 1980\")\n",
        "ax3.set_xlabel(\"Year\")\n",
        "ax3.set_ylabel(\"GDP per capita\")\n",
        "ax3.legend(loc=\"upper center\", bbox_to_anchor=(0.5, -0.20), ncol=3)\n",
        "ax3.grid(True)\n",
        "\n",
        "# plot for population since 1980\n",
